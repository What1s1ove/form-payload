{
  "name": "form-payload",
  "version": "0.0.1",
  "main": "src/index.ts",
  "scripts": {
    "lint:editorconfig": "editorconfig-checker",
    "lint:ts": "eslint --ext .ts src",
    "lint": "npm run lint:editorconfig && npm run lint:ts",
    "build:ts": "tsc && tscpaths -p tsconfig.json -s ./src -o ./build",
    "build": "npm run build:ts && cp package.json build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "editorconfig-checker": "3.3.0",
    "eslint": "7.20.0",
    "lint-staged": "10.5.4",
    "simple-pre-commit": "1.2.1",
    "tscpaths": "0.0.9",
    "typescript": "4.2.2"
  },
  "simple-pre-commit": "npx lint-staged",
  "keywords": [
    "form",
    "form-data",
    "form-payload"
  ],
  "license": "ISC"
}
